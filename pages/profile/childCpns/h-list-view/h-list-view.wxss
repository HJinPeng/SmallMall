/* pages/profile/childCpns/h-list-view/h-list-view.wxss */
.list-view {
  border-top: 36rpx solid #f8f8f8;
}

.list-item {
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  font-size: 32rpx;
  border-bottom: 1px solid #f8f8f8;
  margin-left: 30rpx;
}

.list-item:last-child {
  border: none;
}

.list-item .image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}

.list-item .name {
  margin-left: 16rpx;
}